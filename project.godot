player="*res://player/player.tscn"
ShopItemsList="*res://scripts/shop_items_list.gd"
ChangeControls="*res://scripts/change_controls.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://addons/cursor/pointer.svg"
window/defaults/default_clear_color=Color(0, 0, 0, 1)

[editor]

